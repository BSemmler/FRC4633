#pragma once

class InputOutputClass
{
    void CreateFile(string filePath);
    void OpenFile(string filePath);
    void NewLine();
    void DeleteLine();
    
    string Write(string input);
    string Write(int input);
    string Write(float input);
    string Write(double input);
    
    string WriteToNewLine(string input);
    string WriteToNewLine(int input);
    string WriteToNewLine(float input);
    string WriteToNewLine(double input);
};
